        let userAnswers = {};
        let quizSubmitted = false;

        function renderQuiz() {
            const container = document.getElementById('questionsContainer');
            container.innerHTML = '';

            quizData.forEach((q, index) => {
                const card = document.createElement('div');
                card.className = 'question-card';

                let optionsHTML = '';
                q.options.forEach((option, optIndex) => {
                    optionsHTML += `
                        <div class="option" data-question="${index}" data-option="${optIndex}">
                            <span class="option-label">${String.fromCharCode(65 + optIndex)}.</span>
                            <span class="option-text">${option}</span>
                        </div>
                    `;
                });

                card.innerHTML = `
                    <div class="question-header">
                        <div class="question-number">${index + 1}</div>
                        <div class="question-text">${q.question}</div>
                        <div class="status-icon" id="status-${index}"></div>
                    </div>
                    <div class="options">
                        ${optionsHTML}
                    </div>
                `;

                container.appendChild(card);
            });

            document.querySelectorAll('.option').forEach(option => {
                option.addEventListener('click', handleOptionClick);
            });

            updateSubmitButton();
        }

        function handleOptionClick(e) {
            if (quizSubmitted) return;

            const option = e.currentTarget;
            const questionIndex = parseInt(option.dataset.question);
            const optionIndex = parseInt(option.dataset.option);

            const allOptions = document.querySelectorAll(`[data-question="${questionIndex}"]`);
            allOptions.forEach(opt => opt.classList.remove('selected'));
            
            option.classList.add('selected');
            userAnswers[questionIndex] = optionIndex;

            updateSubmitButton();
        }

        function updateSubmitButton() {
            const submitBtn = document.getElementById('submitBtn');
            submitBtn.disabled = Object.keys(userAnswers).length !== quizData.length;
        }

        function submitQuiz() {
            quizSubmitted = true;
            let score = 0;

            quizData.forEach((q, index) => {
                const allOptions = document.querySelectorAll(`[data-question="${index}"]`);
                const statusIcon = document.getElementById(`status-${index}`);

                allOptions.forEach((option, optIndex) => {
                    option.classList.add('disabled');
                    
                    if (optIndex === q.correct) {
                        option.classList.add('correct');
                    }
                    
                    if (userAnswers[index] === optIndex && optIndex !== q.correct) {
                        option.classList.add('incorrect');
                    }
                });

                if (userAnswers[index] === q.correct) {
                    score++;
                    statusIcon.innerHTML = '‚úì';
                    statusIcon.style.color = '#28a745';
                } else {
                    statusIcon.innerHTML = '‚úó';
                    statusIcon.style.color = '#dc3545';
                }
            });

            const percentage = ((score / quizData.length) * 100).toFixed(1);
            
            document.getElementById('scoreDisplay').textContent = `${score} / ${quizData.length}`;
            document.getElementById('percentageDisplay').textContent = `${percentage}%`;
            
            let message = '';
            if (percentage < 50) {
                message = 'ÿÆŸàŸäŸá ÿ¥ŸàŸÅŸÑŸÉ Ÿàÿßÿ≠ÿØ ÿ™ÿ∫ÿ¥ ŸÖŸÜŸá üôèüèª';
            } else if (percentage >= 50 && percentage < 65) {
                message = 'Ÿàÿ∂ÿπŸÉ ÿ™ŸÖÿßŸÖ ÿßŸÑŸÖŸáŸÖ ŸÖÿßÿ±ÿ≥ÿ®ÿ™ ÿå ŸäŸÑÿß ÿ±Ÿàÿ≠ ŸÉŸÖŸÑ ŸÖÿ±ÿßÿ¨ÿπÿ©';
            } else {
                message = 'ÿ¥ÿπÿ≠ÿ® ÿÆŸàÿ¥ ŸÇÿßÿ±Ÿä ÿßŸÑŸäŸàŸÖ ü§®';
            }
            
            document.getElementById('messageDisplay').textContent = message;
            document.getElementById('resultsBox').classList.add('show');
            document.getElementById('submitBtn').style.display = 'none';
            document.getElementById('resetBtn').classList.add('show');

            window.scrollTo({ top: 0, behavior: 'smooth' });
        }

        function resetQuiz() {
            userAnswers = {};
            quizSubmitted = false;
            
            document.getElementById('resultsBox').classList.remove('show');
            document.getElementById('submitBtn').style.display = 'inline-block';
            document.getElementById('resetBtn').classList.remove('show');
            
            renderQuiz();
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }

        renderQuiz();
    </script>
